    "@tanstack/react-query": "^5.90.2",
    "@ugrc/eslint-config": "^1.2.3",
    "@ugrc/esri-theme-toggle": "^1.1.2",
    "@ugrc/utah-design-system": "^1.30.0",
    "@ugrc/utilities": "^3.1.0",
    "@xstate/react": "^6.0.0",
    "firebase": "^12.3.0",
