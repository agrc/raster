    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.37.0",
    "happy-dom": "^19.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
